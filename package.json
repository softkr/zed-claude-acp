{
  "name": "zed-claude-acp",
  "version": "0.1.0",
  "description": "A Yarn-based ACP server that bridges Zed External Agents to Claude Code",
  "type": "module",
  "main": "dist/cli.js",
  "bin": "dist/cli.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint src",
    "prepublishOnly": "yarn build",
    "prepare": "yarn build"
  },
  "keywords": [
    "acp",
    "zed",
    "claude",
    "agent",
    "ai",
    "external-agent",
    "agent-client-protocol"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/softkr/zed-claude-acp.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "latest",
    "@zed-industries/agent-client-protocol": "0.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.17.0",
    "prettier": "^3.6.2",
    "tsx": "^4.15.0",
    "typescript": "^5.6.0"
  },
  "packageManager": "yarn@4.9.4"
}
